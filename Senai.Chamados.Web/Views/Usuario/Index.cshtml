@model Senai.Chamados.Web.ViewModels.Usuario.ListaUsuarioViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@section Styles{
    link href="http://getbootstrap.com/docs/4.1/examples/starter-template/starter-template.css" rel="stylesheet" />
}
<h2>Index</h2>

<main role="main" class="container">
    <div class="starter-template">
        <h2>Tela do Usuario</h2>
    </div>
</main>
<h2>Tela usuario</h2>

@TempData ["Erro"]
<table class="table">

    <thead>
        <tr>
            <th scope="col">Nome</th>
            <th scope="col">Email</th>
            <th scope="col">Telefone</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model.ListaUsuarios)
        {
            <tr>
                <td>@Html.Display(usuario.Nome)</td>
                <td>@Html.Display(usuario.Email)</td>
                <td>@Html.Display(usuario.Telefone</td>
                <td><a href="/Usuario/EditarUsuario/@usuario.Id">Editar</a></td>
                <td><a href="/Usuario/Editarusuario/@usuario.Id">Deletar</a></td>

            </tr>
        }
    </tbody>
</table>


